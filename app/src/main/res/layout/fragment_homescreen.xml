<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="homescreenVM" type="com.meucomp.app.modules.homescreen.data.viewmodel.HomescreenVM"/>
    </data>
    <LinearLayout android:gravity="start"
        android:id="@+id/linearColumnsearch"
        android:layout_height="match_parent"
        android:layout_marginTop="@dimen/_47pxv"
        android:layout_width="match_parent"
        android:orientation="vertical" style="@style/groupStyleindigo_A200">
        <FrameLayout android:id="@+id/frameHomescreen"
            android:layout_height="match_parent"
            android:layout_width="match_parent" style="@style/groupStyleindigo_A200">
            <HorizontalScrollView android:fillViewport="true"
                android:id="@+id/horizontalScrollHorizontalscrollregistros"
                android:layout_gravity="bottom|start"
                android:layout_height="wrap_content"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="match_parent" android:scrollbars="none">
                <LinearLayout android:gravity="start"
                    android:id="@+id/linearColumnregistros"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:orientation="vertical" style="@style/groupStylewhite_A700cornerRadius">
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtRegistros"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_29pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{homescreenVM.homescreenModel.txtRegistros}"
                        style="@style/txtPoppinsbold24" tools:text="@string/lbl_registros"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtTodasascatego"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_1pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{homescreenVM.homescreenModel.txtTodasascatego}"
                        style="@style/txtPoppinsmedium14" tools:text="@string/msg_todas_as_catego"/>
                    <LinearLayout android:gravity="end"
                        android:id="@+id/linearSlider"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_14pxv"
                        android:layout_width="wrap_content" android:orientation="horizontal">
                        <LinearLayout
                            android:id="@+id/linearRowellipsisvtwo"
                            android:layout_height="wrap_content"
                            android:layout_weight="0.99"
                            android:layout_width="0dp" android:orientation="horizontal">
                            <LinearLayout android:gravity="end"
                                android:id="@+id/linearColumnellipsisvtwo"
                                android:layout_height="wrap_content"
                                android:layout_weight="0.47"
                                android:layout_width="0dp"
                                android:orientation="vertical" style="@style/groupStyleindigo_A700cornerRadius">
                                <ImageView
                                    android:id="@+id/imageEllipsisvTwo"
                                    android:layout_height="@dimen/_12pxh"
                                    android:layout_marginEnd="@dimen/_22pxh"
                                    android:layout_marginStart="@dimen/_22pxh"
                                    android:layout_marginTop="@dimen/_22pxv"
                                    android:layout_width="@dimen/_4pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_ellipsisv2"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipsisv2"/>
                                <TextView
                                    android:gravity="center_horizontal"
                                    android:id="@+id/txtTexto"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:layout_marginEnd="@dimen/_16pxh"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_31pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{homescreenVM.homescreenModel.txtTexto}"
                                    style="@style/txtPoppinssemibold16" tools:text="@string/msg_pensamentos_dis"/>
                            </LinearLayout>
                            <LinearLayout
                                android:id="@+id/linearColumnellipsisvtwoOne"
                                android:layout_height="wrap_content"
                                android:layout_marginStart="@dimen/_16pxh"
                                android:layout_weight="0.47"
                                android:layout_width="0dp"
                                android:orientation="vertical" style="@style/groupStyleindigo_A200cornerRadius">
                                <ImageView
                                    android:id="@+id/imageEllipsisvTwoOne"
                                    android:layout_gravity="end"
                                    android:layout_height="@dimen/_12pxh"
                                    android:layout_marginEnd="@dimen/_22pxh"
                                    android:layout_marginStart="@dimen/_22pxh"
                                    android:layout_marginTop="@dimen/_22pxv"
                                    android:layout_width="@dimen/_4pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_ellipsisv2"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipsisv2"/>
                                <TextView android:gravity="start"
                                    android:id="@+id/txtTextoOne"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_4pxv"
                                    android:layout_marginEnd="@dimen/_16pxh"
                                    android:layout_marginStart="@dimen/_16pxh"
                                    android:layout_marginTop="@dimen/_29pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{homescreenVM.homescreenModel.txtTextoOne}"
                                    style="@style/txtPoppinssemibold16" tools:text="@string/msg_registro_de_emo"/>
                            </LinearLayout>
                            <FrameLayout
                                android:id="@+id/frameStackrectangletwenty"
                                android:layout_height="@dimen/_119pxv"
                                android:layout_marginStart="@dimen/_16pxh" android:layout_width="@dimen/_17pxh">
                                <ImageView
                                    android:id="@+id/imageRectangleTwenty"
                                    android:layout_gravity="end"
                                    android:layout_height="@dimen/_119pxh"
                                    android:layout_width="@dimen/_148pxh"
                                    android:scaleType="fitXY"
                                    cornerRadius="@{@dimen/_16pxh}"
                                    imageRes="@{@drawable/img_rectangle20}"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_rectangle20"/>
                                <TextView android:gravity="top|start"
                                    android:id="@+id/txtRegistrosdeSi"
                                    android:layout_gravity="bottom|end"
                                    android:layout_height="wrap_content"
                                    android:layout_marginBottom="@dimen/_5pxv"
                                    android:layout_marginStart="@dimen/_10pxh"
                                    android:layout_marginTop="@dimen/_10pxv"
                                    android:layout_width="match_parent"
                                    android:text="@{homescreenVM.homescreenModel.txtRegistrosdeSi}"
                                    style="@style/txtPoppinssemibold16" tools:text="@string/msg_registros_de_si"/>
                            </FrameLayout>
                        </LinearLayout>
                        <ImageView android:id="@+id/imageEllipsisvThree"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginBottom="@dimen/_85pxv"
                            android:layout_marginEnd="@dimen/_22pxh"
                            android:layout_marginStart="@dimen/_105pxh"
                            android:layout_marginTop="@dimen/_22pxv"
                            android:layout_width="@dimen/_4pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_ellipsisv3"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipsisv3"/>
                    </LinearLayout>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtSuasTarefasOne"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_26pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{homescreenVM.homescreenModel.txtSuasTarefasOne}"
                        style="@style/txtPoppinsbold24" tools:text="@string/lbl_suas_tarefas"/>
                    <TextView android:ellipsize="end"
                        android:id="@+id/txtPrximastarefa"
                        android:layout_height="wrap_content"
                        android:layout_marginEnd="@dimen/_28pxh"
                        android:layout_marginStart="@dimen/_28pxh"
                        android:layout_marginTop="@dimen/_2pxv"
                        android:layout_width="wrap_content"
                        android:singleLine="true"
                        android:text="@{homescreenVM.homescreenModel.txtPrximastarefa}"
                        style="@style/txtPoppinsmedium14" tools:text="@string/msg_pr_ximas_tarefa"/>
                    <LinearLayout android:gravity="start"
                        android:id="@+id/linearRowestabelecimentoOne"
                        android:layout_height="wrap_content"
                        android:layout_marginBottom="@dimen/_44pxv"
                        android:layout_marginEnd="@dimen/_10pxh"
                        android:layout_marginStart="@dimen/_10pxh"
                        android:layout_marginTop="@dimen/_17pxv"
                        android:layout_width="wrap_content"
                        android:orientation="horizontal" style="@style/groupStyleindigo_A701cornerRadius">
                        <LinearLayout android:gravity="start"
                            android:id="@+id/linearColumnestabelecimentoOne"
                            android:layout_height="wrap_content"
                            android:layout_marginBottom="@dimen/_17pxv"
                            android:layout_marginStart="@dimen/_20pxh"
                            android:layout_marginTop="@dimen/_19pxv"
                            android:layout_weight="0.99"
                            android:layout_width="0dp" android:orientation="vertical">
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtEstabelecimentoOne"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{homescreenVM.homescreenModel.txtEstabelecimentoOne}"
                                style="@style/txtPoppinssemibold16" tools:text="@string/msg_estabelecimento"/>
                            <TextView android:ellipsize="end"
                                android:id="@+id/txtText"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_5pxv"
                                android:layout_width="match_parent"
                                android:singleLine="true"
                                android:text="@{homescreenVM.homescreenModel.txtText}"
                                style="@style/txtPoppinsmedium12" tools:text="@string/msg_escrever_10_met"/>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearRowlocation"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginStart="@dimen/_2pxh"
                                android:layout_marginTop="@dimen/_13pxv"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageLocation"
                                    android:layout_height="@dimen/_13pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginTop="@dimen/_2pxv"
                                    android:layout_width="@dimen/_11pxh"
                                    android:scaleType="fitXY"
                                    android:src="@drawable/img_location"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_location"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtDate"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_12pxh"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{homescreenVM.homescreenModel.txtDate}"
                                    style="@style/txtPoppinsregular12" tools:text="@string/lbl_12_04"/>
                            </LinearLayout>
                            <LinearLayout android:gravity="start"
                                android:id="@+id/linearRowellipsefour"
                                android:layout_height="wrap_content"
                                android:layout_marginEnd="@dimen/_10pxh"
                                android:layout_marginTop="@dimen/_3pxv"
                                android:layout_width="wrap_content" android:orientation="horizontal">
                                <ImageView
                                    android:id="@+id/imageEllipseFour"
                                    android:layout_height="@dimen/_16pxh"
                                    android:layout_marginBottom="@dimen/_1pxv"
                                    android:layout_marginTop="@dimen/_1pxv"
                                    android:layout_width="@dimen/_16pxh"
                                    android:scaleType="centerCrop"
                                    circular="@{true}"
                                    imageRes="@{@drawable/img_ellipse4}"
                                    tools:ignore="ContentDescription" tools:src="@drawable/img_ellipse4"/>
                                <TextView android:ellipsize="end"
                                    android:id="@+id/txtJulieWatson"
                                    android:layout_height="wrap_content"
                                    android:layout_marginStart="@dimen/_6pxh"
                                    android:layout_width="match_parent"
                                    android:singleLine="true"
                                    android:text="@{homescreenVM.homescreenModel.txtJulieWatson}"
                                    style="@style/txtPoppinsregular12_1" tools:text="@string/lbl_julie_watson"/>
                            </LinearLayout>
                        </LinearLayout>
                        <ImageView
                            android:id="@+id/imageEllipsisvTwoTwo"
                            android:layout_height="@dimen/_12pxh"
                            android:layout_marginBottom="@dimen/_98pxv"
                            android:layout_marginEnd="@dimen/_26pxh"
                            android:layout_marginTop="@dimen/_27pxv"
                            android:layout_width="@dimen/_4pxh"
                            android:scaleType="fitXY"
                            android:src="@drawable/img_ellipsisv2"
                            tools:ignore="ContentDescription" tools:src="@drawable/img_ellipsisv2"/>
                    </LinearLayout>
                </LinearLayout>
            </HorizontalScrollView>
            <ImageView android:id="@+id/imageSearch"
                android:layout_gravity="top|start"
                android:layout_height="@dimen/_20pxh"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_41pxh"
                android:layout_marginStart="@dimen/_41pxh"
                android:layout_marginTop="@dimen/_10pxv"
                android:layout_width="@dimen/_20pxh"
                android:scaleType="fitXY"
                android:src="@drawable/img_search"
                tools:ignore="ContentDescription" tools:src="@drawable/img_search"/>
            <ImageView android:id="@+id/imageUndrawfocusse"
                android:layout_gravity="top|end"
                android:layout_height="@dimen/_241pxh"
                android:layout_marginBottom="@dimen/_44pxv"
                android:layout_marginStart="@dimen/_10pxh"
                android:layout_marginTop="@dimen/_44pxv"
                android:layout_width="@dimen/_291pxh"
                android:scaleType="fitXY"
                imageRes="@{@drawable/img_undrawfocusse}"
                tools:ignore="ContentDescription" tools:src="@drawable/img_undrawfocusse"/>
            <TextView android:ellipsize="end"
                android:id="@+id/txtMeuComp"
                android:layout_gravity="top|start"
                android:layout_height="wrap_content"
                android:layout_marginBottom="@dimen/_10pxv"
                android:layout_marginEnd="@dimen/_94pxh"
                android:layout_marginStart="@dimen/_94pxh"
                android:layout_width="wrap_content"
                android:singleLine="true"
                android:text="@{homescreenVM.homescreenModel.txtMeuComp}"
                style="@style/txtPoppinssemibold32" tools:text="@string/lbl_meucomp"/>
        </FrameLayout>
    </LinearLayout>
</layout>
